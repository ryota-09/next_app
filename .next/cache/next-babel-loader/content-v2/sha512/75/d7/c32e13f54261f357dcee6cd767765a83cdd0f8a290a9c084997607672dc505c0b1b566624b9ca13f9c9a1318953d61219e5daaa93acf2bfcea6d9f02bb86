Name(e){\n        this.setState({\n            name_str:e.target.value\n        })\n    }\n\n    doChangeMsg(e){\n        this.setState({\n            msg_str:e.target.value\n        })\n    }\n    doAction(event){\n        this.addFireData();\n        Router.push('fire');\n    }\n\n    //最後のIDを取得\n    getLastID(){\n        let db = firebase.database();\n        let ref = db.ref('sample/');\n        let self = this;\n        ref.orderByKey().limitToLast(1).on('value', (snapshot) => {\n            let res = snapshot.val();\n            for(let i in res){\n                self.setState({\n                    lastID:i\n                });\n                return;\n            }\n        });\n    }\n    //データを追加する\n    addFireData(){\n         if (this.state.lastID == -1){\n            return;\n        }\n        let id = this.state.lastID * 1 + 1;\n        let db = firebase.database();\n        let ref = db.ref('sample/'+ id);\n        ref.set({\n            ID: id,\n            message: this.state.msg_str\n        });\n    }\n\n    //レンダリング\n    render(){\n        if (this.state.lastID == -1){\n            this.getLastID();\n        }\n        return (<div>\n            {(this.state.lastID == -1)\n            ?\n            <p>please wait...</p>\n            :\n            <table>\n            <tbody>\n                <tr>\n                    <th className=\"label\">name</th>\n                    <td><input type=\"text\" placeholder=\"your name.\" onChange={this.doChangeName} value={this.state.name_str} /></td>\n                </tr>\n                <tr>\n                    <th className=\"label\">message</th>\n                    <td><input type=\"text\" size=\"40\" placeholder=\"type message...\" onChange={this.doChangeMsg} value={this.state.msg_str} /></td>\n                </tr>\n                <tr><th></th><td>\n                    <button onClick={this.doAction}>Add</button>    \n                </td></tr>\n            </tbody>\n            </table>\n       